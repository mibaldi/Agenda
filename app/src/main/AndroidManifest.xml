<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mikel.agenda" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />
    <uses-permission android:name="android.permission.MANAGE_ACCOUNTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".ActividadPrincipal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".crear_asignatura"
            android:configChanges="orientation"
            android:label="@string/title_activity_crear_asignatura"
            android:parentActivityName=".ActividadPrincipal"
            android:windowSoftInputMode="stateHidden" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.ActividadPrincipal" />
        </activity>
        <activity
            android:name=".lista"
            android:label="@string/title_activity_lista"
            android:parentActivityName=".ActividadPrincipal"
            android:windowSoftInputMode="stateHidden" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.ActividadPrincipal" />
        </activity>
        <activity
            android:name=".Examenes"
            android:label="@string/title_activity_examenes"
            android:parentActivityName=".ActividadPrincipal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.ActividadPrincipal" />
        </activity>
        <activity
            android:name=".crear_examen"
            android:label="@string/title_activity_crear_examen"
            android:parentActivityName=".ActividadPrincipal"
            android:windowSoftInputMode="stateHidden" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.ActividadPrincipal" />
        </activity>
        <activity
            android:name=".Asignatura"
            android:label="@string/title_activity_asignatura"
            android:parentActivityName=".lista" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.lista" />
        </activity>
        <activity
            android:name=".Examen"
            android:label="@string/title_activity_examen"
            android:parentActivityName=".Examenes" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.Examenes" />
        </activity>
        <activity
            android:name="mycalendar.CalendarSampleActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop" >
        </activity>
        <activity android:name="mycalendar.AddOrEditCalendarActivity" >
        </activity>
        <activity
            android:name=".Contacto"
            android:label="@string/title_activity_contacto"
            android:parentActivityName=".ActividadPrincipal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.ActividadPrincipal" />
        </activity>
        <activity
            android:name=".Modificar_Asignatura"
            android:label="@string/title_activity_modificar__asignatura"
            android:parentActivityName=".Asignatura" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.Asignatura" />
        </activity>
        <activity
            android:name=".Modificar_Examen"
            android:label="@string/title_activity_modificar__examen"
            android:parentActivityName=".Examen" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.Examen" />
        </activity>
        <activity
            android:name="drive.HomeActivity"
            android:label="@string/app_name" />
        <activity android:name="drive.ListFilesActivity" />
        <activity android:name="drive.QueryFilesActivity" />
        <activity android:name="drive.SyncRequestsActivity" />
        <activity android:name="drive.RetrieveContentsActivity" />
        <activity android:name="drive.RetrieveContentsWithProgressDialogActivity" />
        <activity android:name="drive.CreateFileActivity" />
        <activity android:name="drive.CreateFileInAppFolderActivity" />
        <activity android:name="drive.CreateFolderActivity" />
        <activity android:name="drive.EditContentsActivity" />
        <activity android:name="drive.EditMetadataActivity" />
        <activity android:name="drive.CreateFileWithCreatorActivity" />
        <activity android:name="drive.PickFileWithOpenerActivity" />
        <activity android:name="drive.PickFolderWithOpenerActivity" />
        <activity android:name="drive.RetrieveMetadataActivity" />
        <activity android:name="drive.CreateFileInFolderActivity" />
        <activity android:name="drive.CreateFolderInFolderActivity" />
        <activity android:name="drive.ListFilesInFolderActivity" />
        <activity android:name="drive.QueryFilesInFolderActivity" />
        <activity android:name="drive.PinFileActivity" />
        <activity android:name="drive.events.ListenChangeEventsForFilesActivity" />
        <activity android:name="drive.QueryNonTextFilesActivity" />
        <activity android:name="drive.QueryFilesSharedWithMeActivity" />
        <activity android:name="drive.QueryFilesWithAInTitleActivity" />
        <activity android:name="drive.QueryStarredTextFilesActivity" />
        <activity android:name="drive.QueryTextOrHtmlFilesActivity" />
        <activity android:name="drive.InsertUpdateCustomPropertyActivity" />
        <activity android:name="drive.QueryFilesWithCustomPropertyActivity" />
        <activity android:name="drive.DeleteCustomPropertyActivity" />
        <activity android:name="drive.QuerySortedFilesActivity" />
        <activity
            android:name="drive.Backup"
            android:label="@string/app_name" />
        <activity
            android:name=".driveActivity"
            android:label="@string/title_activity_drive"
            android:parentActivityName=".ActividadPrincipal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.ActividadPrincipal" />
        </activity>

        <activity
            android:name="drive.Exportar"
            android:label="@string/app_name"
            android:parentActivityName=".ActividadPrincipal" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.mikel.agenda.ActividadPrincipal"/>
        </activity>
        <activity
            android:name="drive.Importar"
            android:label="@string/app_name"
            android:parentActivityName=".ActividadPrincipal" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mikel.agenda.ActividadPrincipal"/>
        </activity>
    </application>

</manifest>
